<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns:fx="http://javafx.com/fxml"
          prefHeight="2000" prefWidth="3200"
>
    <ScrollPane GridPane.columnIndex="0" GridPane.rowIndex="0">
        <ImageView
                fx:id="gameboard"
                fitHeight="8000"
                fitWidth="1358.25"
                layoutX="2"
                layoutY="2"
                preserveRatio="true"
                GridPane.fillWidth=""
        >
                <Image
                        url="@../images/gameboard.jpeg"/>
        </ImageView>

    </ScrollPane>
    <ScrollPane GridPane.columnIndex="1" GridPane.rowIndex="0">
        <ImageView
                fx:id="card"
                preserveRatio="true"
                fitHeight="100"
                fitWidth="100"

        >
                <Image
                        url="@../images/developmentCards/Commercial_Hub.png"
                />
        </ImageView>
    </ScrollPane>
</GridPane>

